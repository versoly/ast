exports[`files 1`] = `
[
  {
    "content": "<section>
<h1>{header}</h1>
<p>{subheader}</p>
<button class:list={['btn', {btnColor: btnColor, btnSize: btnSize, btnFullWidth: btnFullWidth}]} />

</section>",
    "path": "pages/index.astro",
  },
  {
    "content": "---
interface Props {
  header?: string;
  subheader?: string;
  btnColor?: string;
  btnSize?: string;
  btnFullWidth?: boolean;
}

const { header = "Header", subheader = "Subheader", btnColor = "primary", btnSize = , btnFullWidth = false } = Astro.props;

---
<section>
<h1>{header}</h1>
<p>{subheader}</p>
<button class:list={['btn', {btnColor: btnColor, btnSize: btnSize, btnFullWidth: btnFullWidth}]} />

</section>",
    "path": "components/v-section.astro",
  },
  {
    "content": "---
interface Props {
  header?: string;
  subheader?: string;
  useH2Tag?: boolean;
  classNames?: IClassNames;
}
interface IClassNames {
  root?: string;
  header?: string;
}

const { header = "Header", subheader = "Subheader", useH2Tag = false } = Astro.props;
const classNames = { ...Astro.props.classNames, root: "py-20" };

---
<section class:list={classNames.root}>
{!useH2Tag && <h1 class:list={classNames.header}>{header}</h1>
}
{useH2Tag && <h2 class:list={classNames.header}>{header}</h2>
}
<p>{subheader}</p>

</section>",
    "path": "components/v-section-classnames.astro",
  },
]
`;

